<AnchorPane prefHeight="210.0" prefWidth="278.0" style="-fx-background-image: url('/images/immaginiSetUp/finestraSenaFinestra.png'); -fx-background-repeat: stretch; -fx-background-position: center;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.client.ui.gui.controller.PointsController">
   <children>
      <VBox prefHeight="210.0" prefWidth="278.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <ImageView fx:id="faithImage" fitHeight="88.0" fitWidth="79.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/immaginiSetUp/faithPoints.png" />
                     </image></ImageView>
                  <ImageView fx:id="vicotryImage" fitHeight="88.0" fitWidth="79.0" pickOnBounds="true" preserveRatio="true">
