                        <Image url="@../../images/immaginiSetUp/militaryPoints.png" />
                     </image></ImageView>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="61.0" prefWidth="278.0" spacing="20.0">
               <children>
                  <Label fx:id="faithLabel" alignment="CENTER" prefHeight="38.0" prefWidth="79.0" text="ciao" textAlignment="CENTER" />
