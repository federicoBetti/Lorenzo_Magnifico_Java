                        <Image url="@../../images/immaginiSetUp/victoryPoints.png" />
                     </image></ImageView>
                  <ImageView fx:id="militaryImage" fitHeight="88.0" fitWidth="79.0" pickOnBounds="true" preserveRatio="true">
